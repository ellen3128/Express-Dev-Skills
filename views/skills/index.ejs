<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
</head>
<body>
    <h1>DEV Skills</h1>
    <ul>
      <% if (skills) { %>
        <% skills.forEach(function(s) { %>
          <li>
            <a href="/skills/<%= s.id %>"><%= s.name %></a>
              - 
            <%= s.proficiency %>
          </li>
        <% }); %>
      <% } else { %>
        <p>No skills found.</p>
      <% } %>
      
    </ul>
</body>
</html>
